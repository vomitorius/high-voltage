{
  "version": 2,
  "name": "high-voltage-band",
  "builds": [
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    },
    {
      "src": "/",
      "dest": "/web/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}